model Colors {
  id        Int      @id @default(autoincrement())
  name      String
  colorCode String   @unique @map("color_code")
  kanaName  String   @map("kana_name")
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")

  @@map("colors")
}
