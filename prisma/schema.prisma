generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Colors {
  id        Int    @id @default(autoincrement())
  name      String
  colorCode String @unique @map("color_code")
  kanaName  String @map("kana_name")

  @@map("colors")
}

model FontFamilies {
  id    Int    @id @default(autoincrement())
  name  String
  style String

  @@map("font_families")
}

model HeadParts {
  id   Int    @id @default(autoincrement())
  name String

  @@map("head_parts")
}
