generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Color {
  id        Int    @id @default(autoincrement())
  name      String
  className String @unique @map("class_name")
  kanaName  String @map("kana_name")

  @@map("color")
}
