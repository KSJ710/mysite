generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  age  Int

  @@map("user")
}

model Color {
  id        Int    @id @default(autoincrement())
  name      String
  className String @map("class_name")

  @@map("color")
}
