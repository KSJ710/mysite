generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
  age  Int

  @@map("user")
}

model Template {
  id         Int         @id @default(autoincrement())
  name       String
  layoutTugs LayoutTug[] @relation(references: [id])

  @@map("template")
}

model LayoutTug {
  id       Int        @id @default(autoincrement())
  template Template[] @relation(references: [id])

  @@map("layout_tug")
}
